:$PORT
root * /usr/share/caddy
file_server browse

header {
    X-Robots-Tag none
    X-Content-Type-Options nosniff
    X-Frame-Options DENY
    Referrer-Policy no-referrer-when-downgrade
}

basicauth /65654109-030a-4fbe-916b-34a8448032ae/* {
    65654109-030a-4fbe-916b-34a8448032ae JDJhJDE0JDNNMGFLWUxUWkE1dUxJeEs5MURIWk96Yy82YlU3c0xOZWxGYlFBODdpcW50YlVxeFV4T095
}

@websocket_xms {
    header Connection *Upgrade*
    header Upgrade websocket
    path /65654109-030a-4fbe-916b-34a8448032ae-xms
}
reverse_proxy @websocket_xms unix//etc/caddy/xms
